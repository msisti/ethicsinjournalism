<p id="notice"><%= notice %></p>

<h1>Listing Comment Prompts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @comment_prompts.each do |comment_prompt| %>
      <tr>
        <td><%= link_to 'Show', comment_prompt %></td>
        <td><%= link_to 'Edit', edit_comment_prompt_path(comment_prompt) %></td>
        <td><%= link_to 'Destroy', comment_prompt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Comment prompt', new_comment_prompt_path %>
